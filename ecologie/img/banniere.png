 172 Blumenarten (Orchideen, Polygale, gelbes Sonnenr\u00f6schen...), 39 Insektenarten, darunter die kleine Bergzikade, 28 Vogelarten, 5 S\u00e4ugetierarten und ein Reptil."},"multimedias":[],"moyensCommunications":[{"values":{"de":"+33 3 44 81 10 09","en":"+33 3 44 81 10 09","fr":"+33 3 44 81 10 09"},"type":"tel_1","nom":"Telefon mit Schnur","value":"+33 3 44 81 10 09"},{"values":{"de":"+33 3 22 89 63 96","en":"+33 3 22 89 63 96","fr":"+33 3 22 89 63 96"},"type":"tel_1","nom":"Telefon mit Schnur","value":"+33 3 22 89 63 96"}],"objet_lies":[],"illustrations":{"illustrations":[{"extension":"","fileName":"","copyright":"R. Monnehay \/ CEN Picardie","categorie":"","urlListe":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-010205-ori.JPG","largeur":0,"ordre":1,"alt":[],"urlFiche":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-010205-ori.JPG","created_at":"2023-10-23 04:53:07","type":"Image","nom":{"de":"cenp-010205-ori","en":"cenp-010205-ori","fr":"Le Mont Florentin","es":"cenp-010205-ori"},"url":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-010205-ori.JPG","taille":0,"updated_at":"2023-10-23 04:53:07","urlDiaporama":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-010205-ori.JPG","urlThumbnail":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-010205-ori.JPG","validite_date_fin":null,"hauteur":0},{"extension":"","fileName":"","copyright":"B. Couvreur \/ CEN Picardie","categorie":"","urlListe":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-003638-ori.jpg","largeur":0,"ordre":2,"alt":[],"urlFiche":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-003638-ori.jpg","created_at":"2023-10-23 04:53:07","type":"Image","nom":{"de":"cenp-003638-ori","en":"cenp-003638-ori","fr":"Le Mont Florentin","es":"cenp-003638-ori"},"url":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-003638-ori.jpg","taille":0,"updated_at":"2023-10-23 04:53:07","urlDiaporama":"https:\/\/oise.media.tourinsoft.eu\/upload\/cenp-003638-ori.jpg","urlThumbnail":"https:\/\/oise.media.tourinsoft.eu\/